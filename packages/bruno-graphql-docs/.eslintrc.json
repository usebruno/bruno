{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["../../.eslintrc.json", "plugin:@typescript-eslint/recommended", "plugin:react/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "react"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "@typescript-eslint/ban-types": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
