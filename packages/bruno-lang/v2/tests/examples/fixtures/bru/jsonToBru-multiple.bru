meta {
  name: Multi-Example API
  type: http
}

get {
  url: https://api.example.com/test
}

example {
  name: Example 1
  description: First example
  
  request: {
    url: https://api.example.com/example1
    method: get
    mode: none
  }
}

example {
  name: Example 2
  description: Second example with JSON body
  
  request: {
    url: https://api.example.com/example2
    method: get
    mode: json
    body:json: {
      {
        "data": "test2"
      }
    }
  }
}
